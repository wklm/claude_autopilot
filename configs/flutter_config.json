{
  "comment": "Configuration for Flutter mobile and web app development",
  "tech_stack": "flutter",
  "problem_commands": {
    "analyze": ["flutter", "analyze"],
    "test": ["flutter", "test"]
  },
  "best_practices_files": ["./CLAUDE.md"],
